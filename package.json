{
  "name": "@crawlyfi/moleculer-channels",
  "version": "0.1.8",
  "description": "Reliable messages for Moleculer services",
  "main": "index.js",
  "scripts": {
    "build": "rm -R ./dist || true && tsc --project tsconfig.build.json",
    "dev": "nodemon examples/index.js",
    "check": "tsc --noEmit true",
    "types": "tsc --emitDeclarationOnly true --noEmitOnError false",
    "ci": "jest --watch",
    "test": "jest --coverage --verbose",
    "lint": "eslint --ext=.js src examples test",
    "bench": "node benchmark/index.js",
    "bench:watch": "nodemon benchmark/index.js",
    "deps": "npm-check -u",
    "ci-deps": "ncu --target minor",
    "ci-update-deps": "ncu -u --target minor",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:up": "docker compose -f test/docker-compose.yml up -d",
    "test:down": "docker compose -f test/docker-compose.yml down -v",
    "release": "npm publish --access public && git push --follow-tags"
  },
  "keywords": [
    "moleculer",
    "microservice"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moleculerjs/moleculer-channels.git"
  },
  "typings": "types/index.d.ts",
  "author": "MoleculerJS",
  "license": "MIT",
  "peerDependencies": {
    "moleculer": "^0.14.12"
  },
  "devDependencies": {
    "@types/node": "^18.17.3",
    "amqplib": "^0.10.3",
    "benchmarkify": "^3.0.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "ioredis": "^5.3.2",
    "jest": "^28.1.3",
    "jest-cli": "^28.1.3",
    "kafkajs": "^2.2.4",
    "kleur": "^4.1.5",
    "moleculer": "^0.14.30",
    "moleculer-repl": "^0.7.3",
    "msgpack5": "^6.0.2",
    "nats": "^2.15.1",
    "nodemon": "^2.0.22",
    "npm-check-updates": "^16.10.17",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "roots": [
      "../test"
    ],
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "engines": {
    "node": ">= 12.x.x"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "semver": "^7.5.4"
  }
}